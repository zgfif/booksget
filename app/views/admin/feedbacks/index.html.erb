<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.title %></td>
        <td><%= feedback.text %></td>
        <td><%= link_to 'Show', admin_feedback_url(feedback) %></td>
        <td><%= link_to 'Edit', edit_admin_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', admin_feedback_url(feedback), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

